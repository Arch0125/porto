{
  "rewrites": [
    { "destination": "https://dialog.porto.sh/dialog/", "source": "/dialog/" },
    {
      "destination": "https://:env.dialog.porto.sh/dialog/",
      "has": [
        {
          "type": "host",
          "value": "(?<env>.*)\\..*"
        }
      ],
      "source": "/dialog/"
    },
    {
      "destination": "https://dialog.porto.sh/dialog/:match*",
      "source": "/dialog/:match*"
    },
    {
      "destination": "https://:env.dialog.porto.sh/dialog/:match*",
      "has": [
        {
          "type": "host",
          "value": "(?<env>.*)\\..*"
        }
      ],
      "source": "/dialog/:match*"
    },
    { "destination": "https://manage.porto.sh/", "source": "/" },
    {
      "destination": "https://:env.manage.porto.sh/",
      "has": [
        {
          "type": "host",
          "value": "(?<env>.*)\\..*"
        }
      ],
      "source": "/"
    },
    { "destination": "https://manage.porto.sh/:match*", "source": "/:match*" },
    {
      "destination": "https://:env.manage.porto.sh/:match*",
      "has": [
        {
          "type": "host",
          "value": "(?<env>.*)\\..*"
        }
      ],
      "source": "/:match*"
    }
  ]
}
